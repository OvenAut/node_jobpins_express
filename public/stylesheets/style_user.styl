img
  display block

body
  font-family "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif

#page
  max-width 1100px
  margin 0 auto

#header
  background #333
  text-align left

#title
  color white
  padding 20px
  font-size 3em
  font-family 'Permanent Marker'
  text-shadow 4px 4px 4px #111
  
.titlebox
  box-shadow 0px 0px 5px #111
  z-index 999

#map_canvas
  min-height 400px
  height 400px
  width 1100px
  padding 0
  margin 0
  white-space nowrap
  width 100%
  z-index -999
  box-shadow 0px 0px 5px #111
// index

#content
  //background #333
  //box-shadow 0px 0px 4px #111

//.container
//  padding 0px

#column1
  position relative
  left 25%

#column2
  position relative
  left -75%
#column2 ul
  list-style-type none
#column2 .stackContent li
  background white
  border 1px solid #CCC
  border-radius 5px
  box-shadow 0 0 5px #111
  padding 10px
  margin 5px

#search-list .search-destroy
   position absolute
   right 5px
   top 14px
   display none
   cursor pointer
   width 20px
   height 20px
   background url(images/destroy.png) no-repeat 0 0

#search-list li:hover .search-destroy 
  display block
 
#search-list .search-destroy:hover
  background-position 0 -20px
#column1box 
  background:#fff
  border:1px solid #ccc
  border-radius 5px
  box-shadow:0 0 5px #999
  padding:10px
  margin:5px

#slogen 
  background:orange
  border:1px solid #ccc
  border-radius:5px
  box-shadow:0 0 5px #999
  padding:10px
  margin:5px

#searchColumn
  background:#333
  border:1px solid #ccc
  border-radius:5px
  box-shadow:0 0 5px #999
  padding:4px
  margin:0px
  font-size:2em

input#new-search
  font-family: "Courier New", Courier, mono
  color: #ff2545
  border:1px solid #ccc
  border-radius:5px

#footer
  font-size '0.8em'
  color #CCC
  background #333
  box-shadow 0 2 15px #999
  border-radius 5px
#footer .stackAuto 
  vertical-align middle
  padding 10px 0

#footer .stackContent
  padding 10px
  margin 10px

#footer .stackAuto .stackContent
  margin 0 10px

  /* anchors */
#footer a,
#footer a:visited
  text-decoration:none
  cursor:pointer
  color:#666
  
#footer a:hover,
#footer a:focus
  color:#fff


