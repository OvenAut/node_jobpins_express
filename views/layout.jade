!!! 5
html
  head
    //-link(rel='stylesheet',href='http://fonts.googleapis.com/css?family=Permanent+Marker',type='text/plain')
    link(rel='stylesheet',href='http://fonts.googleapis.com/css?family=Paytone+One',type='text/plain')
    meta(charset='utf-8')
    //-meta(http-equiv="X-UA-Compatible" content="chrome=1")
    //-meta(http-equiv="X-UA-Compatible", content="IE=Edge,chrome=1")
    title= pageTitle
    meta(name="description", content="Jobpins zeigt ")
    meta(name="author", content="Oliver Oszko")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="content-language", content="de")
    link(rel='stylesheet', href='stylesheets/style.css')
    //-.holmes-debug
  body
    #page
      .stack
        #header.titlebox
          .stack3of4
            h1#title.stackContent 
             | Jobpins allways &#x2764; you
            span.edition.stackContent
             | Vienna edition
          .stack1of4
            img(width='88', height='88', src='images/sexy.jpeg', alt='Sexy Title')
        #map_canvas.stackContent
          //map
        #searchapp
          #searchColumn.stackContent
            span.ui-tooltip-bottom Press Enter to save this task
            input#new-search(placeholder='Loading', type='text')
            //-#suggestColumn.stackContent
            ul#suggest-list
          #content
            div#column1.stack3of4
              div.documentdiv
              != body
            div#column2.stack1of4
              div#jobs.stackContent
                ul#search-list 
                 //- for (var key in jobs)
                 //-  li= jobs[key]
              div#search-stats.stackContent
        #footer
          .wrapper
            .stackAuto
              - var year = (new Date).getFullYear()
              p.stackContent &copy; #{year} Oliver Oszko
            .stackAuto
              a.stackContent(href='#') Imprint
            .stackAuto
              a.stackContent(href='http://www.twitter.com/ovenaut') Follow me on Twitter
    // Templates <%= counter? '<span class="search-counter">' + counter + '</span>' : '' %>
    script#suggest-template(type="text/template")
      <div class="display">
      <div class="suggest-content <%= selected ? 'selected' : '' %>"><%= content %></div>
      <span class="gui <%= up ? 'up' : '' %><%= down ? 'down' : '' %>">&#x20;</span>
      </div>
    script#searchItem-template(type="text/template")
      <div class="display" style="background: -webkit-gradient(linear, left top, left bottom, from(<%= color %>), to(#fff));background: -moz-linear-gradient(top, <%= color %>, #fff);">
      <div class="search-content <%= activ? ' activ':''%>"><%= content.link("#!/categories/"+ urlname +"/"+page) %></div>
      <span class="search-destroy"></span>
      <span class="search-counter"><%= counter? counter : 'loading' %>
      </div>
    script#document-template(type="text/template")
      <div class="documentbox stackContent">
      <div class="sidecolor" style="background: -webkit-gradient(linear, 0% 0%, 30 0%, from(<%= color %>), to(#fff));background: -moz-linear-gradient(0deg, <%= color %>, #fff, #fff 50px);">
      <div class="display">
      <div id="navigation">
      <div class="stack1of4">
      <div class="navigationback stackContent" style="background: -webkit-gradient(linear, left top, left bottom, from(<%= color %>), to(#fff));background: -moz-linear-gradient(top, <%= color %>, #fff);">
      <%= index!=last? backText.link(urlname + '/' + last): ''%>
      </div>
      </div>
      <div class="stack1of4">
      <div class="navigationforward stackContent" style="background: -webkit-gradient(linear, left top, left bottom, from(<%= color %>), to(#fff));background: -moz-linear-gradient(top, <%= color %>, #fff);">
      <%= index!=next? forthText.link(urlname + '/' + next): ''%>
      </div>
      </div>
      <div class="companyname">
      <div class="text stackContent"><%= company %></div>
      </div>
      </div>
      <div class="documentbody stackContent"><%= body %></div>
      
      <div id="location"><%= formatted_address %></div>
      </div>
      </div>
      </div>
    script#searchItemOld-template(type="text/template")
      <div class="search <%= done ? 'done' : '' %>">
      <div class="display">
      <input class="check" type="checkbox" <%= done ? 'checked="checked"' : '' %> />
      <div class="search-content"></div>
      <span class="search-destroy"></span>
      </div>
      <div class="edit">
      <input class="search-input" type="text" value="" />
      </div>
      </div>
      
    script#stats-template(type="text/template")
      <% if (total) { %>
      <span class="search-count">
      <span class="number"><%= remaining %></span>
      <span class="word"><%= remaining == 1 ? 'item' : 'items' %></span> left.
      </span>
      <% } %>
      <% if (done) { %>
      <span class="search-clear"> 
      <a href="#">
      Clear <span class="number-done"><%= done %></span>
      completed <span class="word-done"><%= done == 1 ? 'item' : 'items' %></span>
      </a>
      </span>
      <% } %>
    
    script(src="http://maps.google.com/maps/api/js?sensor=false")
    //-script(src="http://ajax.googleapis.com/ajax/libs/chrome-frame/1/CFInstall.min.js")
