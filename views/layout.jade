html(lang="de")
  head
    //-link(rel='stylesheet',href='http://fonts.googleapis.com/css?family=Permanent+Marker',type='text/plain')
    link(rel='stylesheet',href='http://fonts.googleapis.com/css?family=Paytone+One',type='text/plain')
    meta(charset='utf-8')
    //-meta(http-equiv="X-UA-Compatible" content="chrome=1")
    //-meta(http-equiv="X-UA-Compatible", content="IE=Edge,chrome=1")
    title= pageTitle
    meta(name="description", content="Jobpins zeigt auf Google Maps Stellenangebote vom Ams grafisch da")
    meta(name="google-site-verification", content="iKN2yZyVCftauq-qUSTebFpjyMIgAllVx2cI6LEYu6E")
    meta(name="author", content="Oliver Oszko")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    //-meta(http-equiv="content-language", content="DE")
    link(rel='stylesheet', href='stylesheets/style.css')
    //-.holmes-debug
  body
    #page
      .stack
        #header.stackContent
          ///-.stack3of4
          //-#title.stackContent
          span#title Jobpins
          div#slogen
          div#infoel
            ul#info-list
        #map_canvas.stackContent
        #searchapp
          #searchColumn.stackContent
            .zoomWien
              a(href="#!/zoomwien")
                img(width='48', height='48',src='images/Wien_Wappen.png', alt='Wien' ,title='Zoom Wien')
            span.ui-tooltip-bottom Press Enter to save this task
            input#new-search.placeholder(placeholder='Loading', type='text')
            //-#suggestColumn.stackContent
            ul#suggest-list
            .circleMap
              img(width="48",height='48',src='images/circleMapAc2.png',alt="Circle", title='Radius Mode Deactiv')
            .circleMapAc
              img(width="48",height='48',src='images/circleMapAc.png',alt="Circle", title='Radius Mode Activ')
            ul#marker-list
          != body
        #footer.stackAuto
          .wrapper
            .stack1of4
              - var year = (new Date).getFullYear()
              p.stackContent(style="padding-top:10px") &copy; #{year} Oliver Oszko
            .stack1of4
              a.stackContent(href='impressum')(style="padding-top:10px") Impressum
            .stack1of4
              a.stackContent(href='http://www.twitter.com/ovenaut')(style="padding-top:10px") Follow me on Twitter
            .stack1of4
              .donate.stackContent(style="padding-top:7px")
                <form action="https://www.paypal.com/cgi-bin/webscr" method="post">
                <input type="hidden" name="cmd" value="_s-xclick">
                <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
                ">
                <input type="image" src="https://www.paypalobjects.com/de_DE/AT/i/btn/btn_donate_LG.gif" border="0" name="submit" alt="Jetzt einfach, schnell und sicher online bezahlen â€“ mit PayPal.">
                <img alt="" border="0" src="https://www.paypalobjects.com/de_DE/i/scr/pixel.gif" width="1" height="1">
                </form>
    script#suggest-template(type="text/template")
      <div class="display">
      <div class="suggest-content <%= selected ? 'selected' : '' %>"><%= content %></div>
      <span class="gui <%= up ? 'up' : '' %><%= down ? 'down' : '' %>">&#x20;</span>
      </div>
    script#searchItem-template(type="text/template")
      <div class="display" style="background:<%= color %>;background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(<%= color %>));background: -moz-linear-gradient(top, #fff,<%= color %>);">
      <div class="search-content <%= activ? ' activ':''%>"><a href="#!/categories/<%= urlname %>/<%= page %>" title="<%=altText%>"><%= content %></a></div>
      <span class="search-destroy"></span>
      <span class="search-counter"><%= counter? counter : 'loading' %>
      </div>
    script#document-template(type="text/template")
      <div class="documentbox stackContent" style="-box-shadow: 0 0 7px <%= color %>; -moz-box-shadow: 0 0 7px <%= color %>;-khtml-box-shadow: 0 0 7px <%= color %>;-webkit-box-shadow: 0 0 7px <%= color %>">
      <div class="display">
      <div id="navigation">
      <div class="stack1of4">
      <div class="navigationback stackContent" style="background:<%= color %>;background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(<%= color %>));background: -moz-linear-gradient(top, #fff,<%= color %>);">
      <%= index!=last? backText.link(urlname + '/' + last): ''%>
      </div>
      </div>
      <div class="stack1of4">
      <div class="navigationforward stackContent" style="background:<%= color %>;background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(<%= color %>));background: -moz-linear-gradient(top, #fff,<%= color %>);">
      <%= index!=next? forthText.link(urlname + '/' + next): ''%>
      </div>
      </div>
      <div class="companyname" style="border: 1px solid <%= color %>">
      <div class="text stackContent"><%= company %></div>
      </div>
      <div class="jobdata" style="border: 1px solid <%= color %>">
      <div class="text stackContent"><%= employment %></div>
      </div>
      <div class="jobdata" style="border: 1px solid <%= color %>">
      <div class="text stackContent"><%= education %></div>
      </div>
      </div>
      <div class="documentbody stackContent"><%= body %></div>
      <div id="location"><%= formatted_address %></div>
      <div id="location"><%= joblocation %></div>
      </div>
      </div>
    script#info-template(type="text/template")
      <div><%= content %></div>
      <div><%= date %></div>
      <div class="<%= activ? 'activ':'' %>">&nbsp;</div>
    script#radmarkerItem-template(type="text/template")
      <div class="display"style="background:#99A;border-color:#00F">
      <div class='radmarker-email <%= openEdit?"notsee":"" %>'><%= email %></div>
      <span class='radmarker-destroy'></span>
      <input type="email" autocomplete="on" class="radmarker-input <%= !!email&&!openEdit?'notsee':''%><%= !!email?'" value="'+ email +'"':' placeholder" value="'+ placeholder +'"' %> placeholder="<%= placeholder %>"/>
      <div class='infoLat <%= !!email&&!openEdit?"":"notsee" %>'></div>
      <div class='infoLng <%= !!email&&!openEdit?"":"notsee" %>'></div>
      <div class='infoRad <%= !!email&&!openEdit?"":"notsee" %>'></div>
      </div>
