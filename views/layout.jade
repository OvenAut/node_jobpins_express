!!! 5
html
  head
    //-link(rel='stylesheet',href='http://fonts.googleapis.com/css?family=Permanent+Marker',type='text/plain')
    meta(charset='utf-8')
    //-meta(http-equiv="X-UA-Compatible" content="chrome=1")
    //-meta(http-equiv="X-UA-Compatible", content="IE=Edge,chrome=1")
    title= pageTitle
    meta(name="description", content="")
    meta(name="author", content="Oliver Oszko")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel='stylesheet', href='stylesheets/style.css')

  body
    #page
      .stack
        #header.titlebox
          .stack3of4
            h1#title.stackContent Jobpins allways for you ❤
          .stack1of4
            img(width='88', height='88', src='images/sexy.jpeg', alt='Sexy Title')
        #map_canvas.stackContent
          //map
        #searchColumn.stackConten
          input#new-search(placeholder='What you search for?', type='text')
        #content
          != body
        #footer
          .wrapper
            .stackAuto
              - var year = (new Date).getFullYear()
              p.stackContent © #{year} Oliver Oszko
            .stackAuto
              a.stackContent(href='#') Imprint
            .stackAuto
              a.stackContent(href='http://www.twitter.com/ovenaut') Follow me on Twitter
    // Templates 
    script#searchItem-template(type="text/template")
      <div class="todo <%= done ? 'done' : '' %>">
      <div class="display">
      <input class="check" type="checkbox" <%= done ? 'checked="checked"' : '' %> />
      <div class="todo-content"></div>
      <span class="search-destroy"></span>
      </div>
      <div class="edit">
      <input class="todo-input" type="text" value="" />
      </div>
      </div>
      
    script#stats-template(type="text/template")
      <% if (total) { %>
      <span class="todo-count">
      <span class="number"><%= remaining %></span>
      <span class="word"><%= remaining == 1 ? 'item' : 'items' %></span> left.
      </span>
      <% } %>
      <% if (done) { %>
      <span class="todo-clear">
      <a href="#">
      Clear <span class="number-done"><%= done %></span>
      completed <span class="word-done"><%= done == 1 ? 'item' : 'items' %></span>
      </a>
      </span>
      <% } %>
    
    //-script(src="http://maps.google.com/maps/api/js?sensor=false")
    //-script(src="http://ajax.googleapis.com/ajax/libs/chrome-frame/1/CFInstall.min.js")
    /if IE
      script(type="text/javascript", src="http://ajax.googleapis.com/ajax/libs/chrome-frame/1/CFInstall.min.js")
      script
        window.attachEvent("onload", function() {CFInstall.check({ mode: "overlay"});});
    
    script(src='javascripts/json2.min.js')
    script(src='javascripts/underscore-1.1.6.js')
    script(src='javascripts/backbone.js')
    script(src='javascripts/backbone.localStorage.js')
    script(src='javascripts/jquery-1.6.min.js')
    script(src='javascripts/myscript.js')
